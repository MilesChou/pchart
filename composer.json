{
    "name": "phart/phart",
    "description": "The chart library for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.1 | ^8.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-diactoros": "^2.4",
        "mileschou/imgecho": "^0.1.0",
        "mileschou/psr": "^1.3",
        "phpunit/phpunit": "^7.5 | ^8.5 | ^9.3",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/http-client": "^4 | ^5"
    },
    "autoload": {
        "psr-4": {
            "Phart\\": "src/Phart"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
